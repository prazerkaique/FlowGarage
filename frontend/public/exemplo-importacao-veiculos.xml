    <optionalFeatures>Ar condicionado,Direção elétrica,Vidros elétricos,Travas elétricas,Airbag,ABS,Controle de estabilidade,Central multimídia,Câmera de ré,GPS,Teto solar,Banco de couro,Rodas de liga leve,Xenon</optionalFeatures>
    
    <armored>true</armored>
    <auction>true</auction>
