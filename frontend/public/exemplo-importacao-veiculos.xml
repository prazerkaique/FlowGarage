<?xml version="1.0" encoding="UTF-8"?>
<vehicles>
  <!-- Exemplo de veículo completo -->
  <vehicle>
    <!-- Informações Básicas (Obrigatórias) -->
    <category>Carro</category>
    <brand>Toyota</brand>
    <model>Corolla XEi 2.0</model>
    <year>2023</year>
    <modelYear>2023</modelYear>
    <price>95000</price>
    
    <!-- Informações Adicionais -->
    <licensePlate>ABC-1234</licensePlate>
    <color>Branco Pérola</color>
    <mileage>15000</mileage>
    <description>Veículo em excelente estado de conservação, único dono, todas as revisões em dia.</description>
    <observations>Aceita financiamento. Valor à vista com desconto.</observations>
    
    <!-- Especificações Técnicas -->
    <bodyType>Sedan</bodyType>
    <doors>4</doors>
    <transmission>Automático</transmission>
    <steering>Elétrica</steering>
    <fuel>Flex</fuel>
    
    <!-- Opcionais (separados por vírgula) -->
    <optionalFeatures>Ar condicionado,Direção elétrica,Vidros elétricos,Travas elétricas,Airbag,ABS,Controle de estabilidade,Central multimídia,Câmera de ré</optionalFeatures>
    
    <!-- Características Especiais (true/false) -->
    <armored>false</armored>
    <auction>false</auction>
    <ipvaPaid>true</ipvaPaid>
    <licensingUpToDate>true</licensingUpToDate>
    
    <!-- Status do Veículo -->
    <status>Disponível</status>
    
    <!-- Fotos (nomes dos arquivos no ZIP) -->
    <photos>
      <photo>toyota_corolla_001.jpg</photo>
      <photo>toyota_corolla_002.jpg</photo>
      <photo>toyota_corolla_003.jpg</photo>
      <photo>toyota_corolla_004.jpg</photo>
      <photo>toyota_corolla_005.jpg</photo>
    </photos>
    
    <!-- Vídeos (opcional) -->
    <videos>
      <video>toyota_corolla_video.mp4</video>
    </videos>
  </vehicle>

  <!-- Exemplo de veículo mínimo (apenas campos obrigatórios) -->
  <vehicle>
    <category>Moto</category>
    <brand>Honda</brand>
    <model>CB 600F Hornet</model>
