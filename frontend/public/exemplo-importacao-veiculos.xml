    <photos>
      <photo>honda_hornet_001.jpg</photo>
      <photo>honda_hornet_002.jpg</photo>
    </photos>
  </vehicle>

  <!-- Exemplo de veículo com características especiais -->
  <vehicle>
    <category>Carro</category>
    <brand>BMW</brand>
    <model>X5 xDrive35i</model>
    <year>2019</year>
    <modelYear>2019</modelYear>
    <price>180000</price>
