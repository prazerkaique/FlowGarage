    <photos>
      <photo>bmw_x5_001.jpg</photo>
      <photo>bmw_x5_002.jpg</photo>
      <photo>bmw_x5_003.jpg</photo>
      <photo>bmw_x5_004.jpg</photo>
      <photo>bmw_x5_005.jpg</photo>
      <photo>bmw_x5_006.jpg</photo>
    </photos>
